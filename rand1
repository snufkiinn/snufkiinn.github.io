var imageArray = ["mywork/img_1.jpg","mywork/img_2.jpg","mywork/img_3.jpg","mywork_4.jpg","mywork_5.jpg","mywork_6.jpg"];

function getRandomImage() {
	var randomNumber = Math.floor(Math.random() * 6) + 1;
	var img = "mywork/img_" + randomNumber + ".jpg";
	document.getElementById("randImage").textContent = ('<img src="' + 'images/random/' + img + '" width="250px">')